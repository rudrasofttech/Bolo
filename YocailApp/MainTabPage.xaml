<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:YocailApp"
            x:Class="YocailApp.MainTabPage" xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom">
    <NavigationPage Title="Home">
        
        <x:Arguments>
            <local:MainPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Search">
        <x:Arguments>
            <local:SearchPage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Explore">
        <x:Arguments>
            <local:DiscoverPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
