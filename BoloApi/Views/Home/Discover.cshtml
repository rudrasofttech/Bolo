
@{
    ViewData["Title"] = "Discover";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div id="explorecont">

    </div>

@section scripts{

        <environment include="Development">
            <script type="text/babel">
                ReactDOM.render(
                <MemberPostList search="explore" viewMode={1} viewModeAllowed="true" />,
            document.getElementById('explorecont')
                );
            </script>
        </environment>
        <environment include="Staging,Production" exclude="Development">
            <script>
                ReactDOM.render( React.createElement(MemberPostList, {
                search: "explore", viewMode: 1, viewModeAllowed: true
}), document.getElementById('explorecont'));
            </script>
        </environment>
}