@{
    ViewData["Title"] = "Conversation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts{
    <environment include="Development">
        <script type="text/babel">
            ReactDOM.render(<Conversation />,document.getElementById('root'));
        </script>
    </environment>
    <environment include="Staging,Production" exclude="Development">
        <script>
            ReactDOM.render(React.createElement(Conversation, { }), document.getElementById('root'));
        </script>
    </environment>
}